#version 330 core

// Ouput data
layout(location = 0) out vec3 FragColor;
layout(location = 1) out vec4 secondColor;
// declare the "in" parameter passed from our vertex shader here (same name and type with "in" instead of "out")
in vec3 color;

void main()
{
	FragColor = vec3(1,1,1);
	secondColor = vec4(FragColor.rgb,1);
	
}